<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
	<PackageId>GmadSharp</PackageId>
	<Version>1.0.0.0</Version>
	<Authors>WorstAquaPlayer</Authors>
	<Description>Library made with YARHL for the .GMA format</Description>
	<PackageTags>gmod gmad gma yarhl</PackageTags>
	<PackageProjectUrl>https://github.com/WorstAquaPlayer/GmadSharp</PackageProjectUrl>
	<RepositoryUrl>https://github.com/WorstAquaPlayer/GmadSharp</RepositoryUrl>
	<RepositoryType>git</RepositoryType>
	<PackageLicenseFile>LICENSE.md</PackageLicenseFile>
	<PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Crc32.NET" Version="1.2.0" />
    <PackageReference Include="Yarhl" Version="4.0.0-preview.157" />

	<Content Include="..\LICENSE.md"
             Link="LICENSE.md"
             Pack="true"
             PackagePath="LICENSE.md"/>
  </ItemGroup>

</Project>
